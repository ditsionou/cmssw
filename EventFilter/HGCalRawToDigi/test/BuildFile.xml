<bin file="HGCalUnpackerTest.cc" name="HGCalUnpacker">
  <use name="DataFormats/HGCalDigi"/>
  <use name="FWCore/MessageLogger"/>
  <use name="EventFilter/HGCalRawToDigi"/>
</bin>
<test name="emul_emptyECOND" command="cmsRun ${LOCALTOP}/src/EventFilter/HGCalRawToDigi/test/emulation_and_unpacking_cfg.py mode=empty"/>
