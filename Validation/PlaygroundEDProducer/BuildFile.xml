  <flags ALPAKA_BACKENDS="1"/>
</bin>

<bin name="alpakaTestKernel" file="alpaka/RecHitCalibration.dev.cc">
  <use name="alpaka"/>
  <use name="catch2"/>
  <use name="HeterogeneousCore/AlpakaInterface"/>
  <flags ALPAKA_BACKENDS="1"/>
</bin>
