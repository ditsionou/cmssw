<use name="FWCore/Framework"/>
<use name="FWCore/ParameterSet"/>
<use name="FWCore/PluginManager"/>
<use name="FWCore/Utilities"/>
<use name="DQMServices/Core"/>
<flags EDM_PLUGIN="1"/>

<bin name="alpakaTestVec" file="alpaka/RecHitCalibration.cc">
  <use name="alpaka"/>
